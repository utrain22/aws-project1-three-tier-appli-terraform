pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello'
            }
        }
         stage('build') {
            steps {
                echo 'build'
            }
        }
         stage('test') {
            steps {
                echo 'test'
            }
        }
         stage('deployed') {
            steps {
                echo 'deployed'
            }
        }
    }
}
